{
    "name": "Requests",
    "components": [
        {
            "type": "LineGraphAndCounter",
            "title": "Requests/Active Sessions",
            "summaryText": "Requests/Active Sessions per minute on Moonstick",
            "dataSource": "elasticsearch",
            "host": "http://logs.laterooms.com:9200",
            "aggregateProperty": "sessionsbytime",
            "valueProperty": [ 
              { "value": "moonstick.doc_count", "property": "moonstick" }, 
              { "value": "legacy.doc_count", "property": "legacy" }
            ],
            "timeProperties": [
              "query.filtered.filter.bool.must.0.range.@timestamp.from",
              "aggs.sessionsbytime.date_histogram.extended_bounds.min"
            ],
            "intervalProperties": [
              "aggs.sessionsbytime.date_histogram.interval"
            ],
            "query": {
               "query":{
                  "filtered":{
                     "filter":{
                        "bool":{
                           "must":[
                              {
                                "range":{
                                  "@timestamp":{
                                    "from":"now-60m"
                                  }
                                }
                              },
                              {
                                  "term": {
                                      "type": "lr_varnish_request"
                                  }
                              },
                              {
                                  "term": {
                                      "url_page_type": "home"
                                  }
                              }
                           ]
                        }
                     }
                  }
               },
               "size":0,
                "aggs":{
                    "sessionsbytime" : {
                        "date_histogram" : {
                            "min_doc_count": 0,
                            "extended_bounds" : {
                                "min" : "now-60m",
                                "max" : "now"
                            },
                            "field" : "@timestamp",
                            "interval" : "1m"
                        },
                        "aggs": {
                          "legacy": {
                            "filter": {
                                "term": {
                                    "is_moonstick": false
                                }
                            }
                          },
                          "moonstick": {
                            "filter": {
                                "term": {
                                    "is_moonstick": true
                                }
                            }
                          }
                        }
                    }
                }
            },
            "graph": {
              "window": {
                "skip": 0,
                "take": 60
              },
              "lines": [
                { "id": "moonstick", "color": "green", "value": "moonstick" },
                { "id": "legacy", "color": "red", "value": "legacy" }
              ]
            },
            "counter": {
                "title": "Sessions per min (Last whole)",
                "className": "commission-graph-counter",
                "window": {
                  "skip": 1,
                  "take": 10
                },
                "precision": 0,
                "counters": [
                  { "id": "moonstick", "color": "green", "value": "moonstick", "text": "Moonstick" },
                  { "id": "legacy", "color": "red", "value": "legacy", "text": "Legacy" }
                ]
            }
        },
        {
            "type": "LineGraphAndCounter",
            "title": "Page Load Time (50th Percentile)",
            "summaryText": "Page Load time for the 50th percentile per minute on Moonstick (WEB)",
            "dataSource": "elasticsearch",
            "host": "http://logs.laterooms.com:9200",
            "aggregateProperty": "responsetime",
            "propertyProcessor": {
              "type": "percentiles"
            },
            "timeProperties": [
              "query.filtered.filter.bool.must.0.range.@timestamp.from",
              "aggs.responsetime.date_histogram.extended_bounds.min"
            ],
            "intervalProperties": [
              "aggs.responsetime.date_histogram.interval"
            ],
            "query": {
              "query": {
                "filtered": {
                  "filter": {
                    "bool": {
                      "must": [
                        {
                          "range" : {
                            "@timestamp" : {
                                "from" : "now-60m"
                            }
                          }
                        },
                        {
                            "term": {
                                "type": "lr_varnish_request"
                            }
                        },
                        {
                            "term": {
                                "tags": "beacon"
                            }
                        },
                        {
                            "term": {
                                "is_moonstick": true
                            }
                        }
                      ]
                    }
                  }
                }
              },
              "aggs": {
                    "responsetime" : {
                        "date_histogram" : {
                            "min_doc_count": 0,
                            "extended_bounds" : {
                                "min" : "now-60m",
                                "max" : "now"
                            },
                            "field" : "@timestamp",
                            "interval": "1m"
                        },
                        "aggs": {
                            "page_complete": {
                                "stats": {
                                    "field": "timing_done"
                                }
                            },
                            "percentiles": {
                                "percentiles": {
                                    "field": "timing_done"
                                }
                            }
                        }
                    }
              },
              "size": 0
            },
            "graph": {
              "window": {
                "skip": 0,
                "take": 60
              },
              "lineColor": "purple"
            },
            "counter": {
                "title": "Average Time to Page Load",
                "className": "counter-smallest-text",
                "type": "average",
                "window": {
                  "skip": 1,
                  "take": 10
                },
                "suffix": "ms",
                "precision": 0
            }
        }
    ]
}
